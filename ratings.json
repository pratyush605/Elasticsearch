{
    "settings": {
        "analysis": {
            "analyzer": {
                "case_insensitive": {
                    "type": "custom",
                    "tokenizer": "keyword",
                    "filter": [
                        "lowercase"
                    ]
                }
            },
            "normalizer": {
                "lowercase_normalizer": {
                    "type": "custom",
                    "filter": [
                        "lowercase"
                    ]
                }
            }
        }
    },
    "mappings": {
        "dynamic_templates": [
            {
                "en": {
                    "match_mapping_type": "string",
                    "match": "*",
                    "mapping": {
                        "type": "text",
                        "analyzer": "english",
                        "fields": {
                            "keyword": {
                                "type": "keyword",
                                "ignore_above": 256
                            }
                        }
                    }
                }
            }
        ],
        "properties": {
            "ACTIVE": {
                "type": "long"
            },
            "ADDDATE": {
                "type": "date",
                "format": "strict_date_optional_time"
            },
            "CREATED_AT": {
                "type": "date",
                "format": "strict_date_optional_time"
            },
            "ID": {
                "type": "long"
            },
            "MEMBER_ID": {
                "type": "long"
            },
            "MODDATE": {
                "type": "date",
                "format": "strict_date_optional_time"
            },
            "NOT_ANALYZED_CATEGORY_NAME": {
                "type": "keyword",
                "normalizer": "lowercase_normalizer"
            },
            "RANKER_ID": {
                "type": "long"
            },
            "RATING": {
                "type": "long"
            },
            "TEXT": {
                "type": "text",
                "analyzer": "english"
            },
            "TITLE": {
                "type": "text",
                "analyzer": "english"
            }
        }
    }
}