{
  "settings": {
    "analysis": {
      "analyzer": {
        "case_insensitive": {
          "type": "custom",
          "tokenizer": "keyword",
          "filter": [
            "lowercase"
          ]
        }
      },
      "normalizer": {
        "lowercase_normalizer": {
          "type": "custom",
          "filter": [
            "lowercase"
          ]
        }
      }
    }
  },
  "mappings": {
    "dynamic_templates": [
      {
        "en": {
          "match_mapping_type": "string",
          "match": "*",
          "mapping": {
            "type": "text",
            "analyzer": "english",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          }
        }
      }
    ],
    "properties": {
      "ADDDATE": {
        "type": "date",
        "format": "strict_date_optional_time"
      },
      "CATG_CODE": {
        "type": "keyword"
      },
      "CATG_TYPE": {
        "type": "keyword"
      },
      "COMMUNITY_ID": {
        "type": "long"
      },
      "COMMUNITY_NAME": {
        "type": "text",
        "analyzer": "english",
        "fields": {
          "keyword": {
            "type": "keyword"
          }
        }
      },
      "CONTINENT_ID": {
        "type": "keyword"
      },
      "COUNTRY_CODE": {
        "type": "keyword"
      },
      "DOCUMENT_ID": {
        "type": "text",
        "analyzer": "english",
        "fields": {
          "keyword": {
            "type": "keyword"
          }
        }
      },
      "FLAG": {
        "type": "long"
      },
      "ICON": {
        "type": "text",
        "analyzer": "english",
        "fields": {
          "keyword": {
            "type": "keyword"
          }
        }
      },
      "INFO": {
        "properties": {
          "backgroundImage": {
            "type": "text",
            "analyzer": "english"
          },
          "description": {
            "type": "text",
            "analyzer": "english"
          },
          "sections": {
            "properties": {
              "cards": {
                "properties": {
                  "content": {
                    "type": "text",
                    "analyzer": "english"
                  },
                  "title": {
                    "type": "text",
                    "analyzer": "english"
                  }
                }
              },
              "title": {
                "type": "text",
                "analyzer": "english"
              }
            }
          },
          "title": {
            "type": "text",
            "analyzer": "english"
          }
        }
      },
      "LOGIN": {
        "type": "keyword"
      },
      "MEMLOGIN": {
        "type": "keyword"
      },
      "MODDATE": {
        "type": "date",
        "format": "strict_date_optional_time"
      },
      "NOT_ANALYZED_CATEGORY_NAME": {
        "type": "keyword",
        "normalizer": "lowercase_normalizer"
      },
      "NOT_ANALYZED_CATG_NAME": {
        "type": "keyword"
      },
      "NOT_ANALYZED_CITY_NAME": {
        "type": "keyword",
        "normalizer": "lowercase_normalizer"
      },
      "NOT_ANALYZED_COMMUNITY_NAME": {
        "type": "keyword"
      },
      "NOT_ANALYZED_COUNTRY_NAME": {
        "type": "keyword",
        "normalizer": "lowercase_normalizer"
      },
      "NOT_ANALYZED_GRANDPARENT_CATG_NAME": {
        "type": "keyword"
      },
      "NOT_ANALYZED_MAIN_CATEGORY": {
        "type": "keyword"
      },
      "NOT_ANALYZED_PARENT_CATG_NAME": {
        "type": "keyword"
      },
      "NOT_ANALYZED_STATE_NAME": {
        "type": "keyword",
        "normalizer": "lowercase_normalizer"
      },
      "SENDEREMAIL": {
        "type": "keyword"
      }
    }
  }
}